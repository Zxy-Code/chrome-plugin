{
  "manifest_version": 2,
  "name": "ZiXi",
  "description": "二维码转换器",
  "version": "1.0",
  "icons": {
    "16": "src/img/logo.png",
    "48": "src/img/logo.png",
    "128": "src/img/logo.png"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "src/js/background.js"
    ]
  },
  "browser_action": {
    "default_name": "子兮二维码",
    "default_icon": "/src/img/logo.png",
    "default_title":"子兮",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "MacCtrl+Shift+Z"
      },
      "description": "打开子兮二维码"
    }
  }
}